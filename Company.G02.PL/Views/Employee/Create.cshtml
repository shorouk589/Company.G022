@model CreateEmployeeDto
@{
    ViewData["Title"] = "Create";
}

<h1>Create Employee</h1>
<br />
<br />

<form asp-action="Create" method="post">
    <div class="form-group my-3">
        <label asp-for="Name"></label>
        <input asp-for="Name" class="form-control" />
        <span asp-validation-for="Name" class="text-danger"></span>
    </div>
    <div class="form-group my-3">
        <label asp-for="Age"></label>
        <input asp-for="Age" class="form-control" />
        <span asp-validation-for="Age" class="text-danger"></span>
    </div>
    <div class="form-group my-3">
        <label asp-for="Email"></label>
        <input asp-for="Email" class="form-control" />
        <span asp-validation-for="Email" class="text-danger"></span>
    </div>
    <div class="form-groupmy-3">
        <label asp-for="Address"></label>
        <input asp-for="Address" class="form-control" />
        <span asp-validation-for="Address" class="text-danger"></span>
    </div>
    <div class="form-group my-3">
        <label asp-for="Phone"></label>
        <input asp-for="Phone" class="form-control" />
        <span asp-validation-for="Phone" class="text-danger"></span>
    </div>
    <div class="form-group my-3">
        <label asp-for="Salary"></label>
        <input asp-for="Salary" class="form-control" />
        <span asp-validation-for="Salary" class="text-danger"></span>
    </div>
    <div class="form-group my-3">
        <label asp-for="IsActive"></label>
        <input asp-for="IsActive" class="form-check" />
      
    </div>
    <div class="form-group my-3">
        <label asp-for="IsDeleted"></label>
        <input asp-for="IsDeleted" class="form-check" />
       
    </div>
    <div class="form-group my-3">
        <label asp-for="HiringDate"></label>
        <input asp-for="HiringDate" class="form-control" />
        <span asp-validation-for="HiringDate" class="text-danger"></span>

    </div>
    <div class="form-group">
        <label asp-for="CreateAt"></label>
        <input asp-for="CreateAt" class="form-control" />
        <span asp-validation-for="CreateAt" class="text-danger"></span>

    </div>
    <div class="my-3">
        <input type="submit" value="Create" class="btn btn-primary" />
        <a asp-action="Index" class="btn btn-light ">
            Back To List
        </a>
    </div>
</form>
