@model Employee
@{
    ViewData["Title"] = "Edit";
}

<h1>Update Employees</h1>
<br />
<br />
<form asp-action="Edit">
    <div class="form-group my-3">
        <label asp-for="Name" class="col-form-label"></label>
        <input asp-for="Name" class="form-control" />
    </div>
    <div class="form-group my-3">
        <label asp-for="Age" class="col-form-label"></label>
        <input asp-for="Age" class="form-control" />
    </div>
    <div class="form-group my-3">
        <label asp-for="Email" class="col-form-label"></label>
        <input asp-for="Email" class="form-control" />
    </div>
    <div class="form-group my-3">
        <label asp-for="Address" class="col-form-label"></label>
        <input asp-for="Address" class="form-control" />
    </div>
    <div class="form-group my-3">
        <label asp-for="Phone" class="col-form-label"></label>
        <input asp-for="Phone" class="form-control" />
    </div>
    <div class="form-group my-3">
        <label asp-for="Salary" class="col-form-label"></label>
        <input asp-for="Salary" class="form-control" />
    </div>
    <div class="form-group my-3">
        <label asp-for="IsActive" class="col-form-label"></label>
        <input asp-for="IsActive" class="form-check" />
    </div>
    <div class="form-group my-3">
        <label asp-for="IsDeleted" class="col-form-label"></label>
        <input asp-for="IsDeleted" class="form-check" />
    </div>
    <div class="form-group my-3">
        <label asp-for="DateOfBirth" class="col-form-label"></label>
        <input asp-for="DateOfBirth" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="CreateAt" class="col-form-label"></label>
        <input asp-for="CreateAt" class="form-control" readonly />
    </div>
    <button type="submit" class="btn btn-primary">Update</button>

    </form>